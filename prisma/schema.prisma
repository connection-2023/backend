  kftCode                   String
  krCode                    String
  enCode                    String
  virtualAccountPaymentInfo VirtualAccountPaymentInfo[]
  userBankAccount           UserBankAccount[]
  lecturerBankAccount       LecturerBankAccount[]
}

model RefundStatus {
