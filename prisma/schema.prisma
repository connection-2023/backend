  lectureId              Int
  day                    String[]                 @default([])
  dateTime               String[]                 @default([])
  numberOfParticipants   Int                      @default(0)
  lecture                Lecture                  @relation(fields: [lectureId], references: [id], onDelete: Cascade)
  regularLectureSchedule RegularLectureSchedule[]
  reservation            Reservation[]
}

model RegularLectureSchedule {
  id                     Int                  @id @default(autoincrement())
  regularLectureStatusId Int
  startDateTime          DateTime
  endDateTime            DateTime
  day                    Int
  regularLectureStatus   RegularLectureStatus @relation(fields: [regularLectureStatusId], references: [id], onDelete: Cascade)
}
