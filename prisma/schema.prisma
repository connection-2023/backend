//신고 유형 
model ReportType {
  id                 Int                  @id @default(autoincrement())
  name               String
  description        String
  userReportType     UserReportType[]
  lecturerReportType LecturerReportType[]
}

//유저가 신고한 내역
model UserReport {
  id               Int      @id @default(autoincrement())
  reportedUserId   Int
  targetUserId     Int?
  targetLecturerId Int?
  reason           String?
  isAnswered       Boolean  @default(false)
  createdAt        DateTime @default(now()) @db.Timestamp(6)
  updatedAt        DateTime @default(now()) @updatedAt @db.Timestamp(6)

  reportedUser       Users               @relation("ReportedUser", fields: [reportedUserId], references: [id])
  targetUser         Users?              @relation("TargetUser", fields: [targetUserId], references: [id])
  targetLecturer     Lecturer?           @relation(fields: [targetLecturerId], references: [id])
  userReportedReview UserReportedReview?
  userReportType     UserReportType[]
  userReportResponse UserReportResponse?
}

//유저가 신고한 리뷰 내역
model UserReportedReview {
  id               Int     @id @default(autoincrement())
  reportId         Int     @unique()
  lectureReviewId  Int?
  lecturerReviewId Int?
  description      String?

  report         UserReport      @relation(fields: [reportId], references: [id])
  lectureReview  LectureReview?  @relation(fields: [lectureReviewId], references: [id])
  lecturerReview LecturerReview? @relation(fields: [lecturerReviewId], references: [id])
}

model UserReportType {
  id           Int @id @default(autoincrement())
  reportId     Int
  reportTypeId Int

  report     UserReport @relation(fields: [reportId], references: [id])
  reportType ReportType @relation(fields: [reportTypeId], references: [id])
}

//강사가 신고한 내역
model LecturerReport {
  id                 Int      @id @default(autoincrement())
  reportedLecturerId Int
  targetUserId       Int?
  targetLecturerId   Int?
  reason             String?
  isAnswered         Boolean  @default(false)
  createdAt          DateTime @default(now()) @db.Timestamp(6)
  updatedAt          DateTime @default(now()) @updatedAt @db.Timestamp(6)

  reportedLecturer       Lecturer                @relation("ReportedLecturer", fields: [reportedLecturerId], references: [id])
  targetUser             Users?                  @relation(fields: [targetUserId], references: [id])
  targetLecturer         Lecturer?               @relation("TargetLecturer", fields: [targetLecturerId], references: [id])
  lecturerReportedReview LecturerReportedReview?
  lecturerReportType     LecturerReportType[]
  lecturerReportResponse LecturerReportResponse?
}

//강사가 신고한 리뷰 내역
model LecturerReportedReview {
  id               Int     @id @default(autoincrement())
  reportId         Int     @unique()
  lectureReviewId  Int?
  lecturerReviewId Int?
  description      String?

  report         LecturerReport  @relation(fields: [reportId], references: [id])
  lectureReview  LectureReview?  @relation(fields: [lectureReviewId], references: [id])
  lecturerReview LecturerReview? @relation(fields: [lecturerReviewId], references: [id])
}

model LecturerReportType {
  id           Int @id @default(autoincrement())
  reportId     Int
  reportTypeId Int

  report     LecturerReport @relation(fields: [reportId], references: [id])
  reportType ReportType     @relation(fields: [reportTypeId], references: [id])
}

model LecturerReportResponse {
